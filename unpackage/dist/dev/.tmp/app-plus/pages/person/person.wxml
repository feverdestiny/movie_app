<view><view class="{{['header',(isH5Plus)?'status':'']}}"><view class="userinfo"><view class="face"><image src="{{userinfo.face}}"></image></view><view class="info"><view class="username">{{userinfo.username}}</view><view class="integral">{{"积分:"+userinfo.integral}}</view></view></view><view class="setting"><image src="../../static/HM-PersonalCenter/setting.png"></image></view></view><view hidden="{{!(false)}}" class="orders"><view class="box"><block wx:for="{{orderTypeLise}}" wx:for-item="row" wx:for-index="index" wx:key="name"><view class="label" hover-class="hover" data-event-opts="{{[['tap',[['toOrderType',[index]]]]]}}" bindtap="__e"><view class="icon"><block wx:if="{{row.badge>0}}"><view class="badge">{{row.badge}}</view></block><image src="{{'../../static/HM-PersonalCenter/'+row.icon}}"></image></view>{{''+row.name+''}}</view></block></view></view><block wx:for="{{severList}}" wx:for-item="list" wx:for-index="list_i" wx:key="list_i"><view class="list"><block wx:for="{{list}}" wx:for-item="li" wx:for-index="li_i" wx:key="name"><view class="{{['li',(li_i==list.length-1)?'noborder':'']}}" hover-class="hover" data-event-opts="{{[['tap',[['toPage',['$0'],[[['severList','',list_i],['','name',li.name]]]]]]]}}" bindtap="__e"><view class="icon"><base-icon vue-id="{{'1-'+list_i+'-'+li_i}}" fontsize="40upx" icon="{{li.icon}}" bind:__l="__l"></base-icon></view><view class="text">{{li.name}}</view><block wx:if="{{li.name==='清除缓存'}}"><view class="text2">{{currentSize}}</view></block><base-icon vue-id="{{'2-'+list_i+'-'+li_i}}" fontsize="35upx" icon="next" bind:__l="__l"></base-icon></view></block></view></block></view>