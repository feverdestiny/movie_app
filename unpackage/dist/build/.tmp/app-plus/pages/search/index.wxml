<view class="wrapper"><block wx:if="{{isHistory}}"><view class="history-box"><block wx:if="{{historyList.length>0}}"><view><view class="history-title"><text>搜索历史</text><base-icon vue-id="1" fontsize="40upx" icon="shanchu" data-event-opts="{{[['^click',[['clearSearch']]]]}}" bind:click="__e" bind:__l="__l"></base-icon></view><view class="history-content"><block wx:for="{{historyList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['listTap',['$0'],[[['historyList','',index]]]]]]]}}" class="history-item" bindtap="__e">{{''+item.vod_name+''}}</view></block></view></view></block><block wx:else><view class="no-data">您还没有历史记录</view></block></view></block><block wx:else><view class="history-box"><block wx:if="{{historyList.length>0}}"><view class="history-list-box"><block wx:for="{{historyList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['listTap',['$0'],[[['historyList','',index]]]]]]]}}" class="history-list-item" bindtap="__e"><rich-text nodes="{{item.vod_name}}"></rich-text></view></block></view></block><block wx:else><view class="no-data">没有搜索到相关内容</view></block></view></block></view>